{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ESNext",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "noImplicitAny": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "out",
        "baseUrl": "."
    },
    "include": ["src/**/*"]
}
